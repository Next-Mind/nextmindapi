# LIGA O REWRITE DE URLS
RewriteEngine on

# REDIRECIONA /public_html PARA A RAIZ DO DOM√çNIO
RewriteCond %{THE_REQUEST} /public_html/([^\s?]*) [NC]
RewriteRule ^public_html/(.*)$ /$1 [L,R=301]

# REGRA DE REWRITE TUDO PARA INDEX
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^(.*)$ ./index.php [QSA,NC]